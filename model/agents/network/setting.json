{
  "enabled": true,
  "name": "Network Monitor Agent",
  "type": "Network",
  "metrics": [
    {
      "name": "network_availability",
      "comparison": "==",
      "critical_value": "0"
    },
    {
      "name": "inet_throughput",
      "comparison": ">=",
      "critical_value": "1000"
    }
  ],
  "interval": 60
}
